<!DOCTYPE mapper 
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.bbs.service.BoardMapper">
	<insert id="insert" parameterType="com.example.demo.bbs.service.Board">
		insert into boards(content, writer_id, reg_date) 
        values (#{content}, #{writerId}, #{regDate})
	</insert>
</mapper>